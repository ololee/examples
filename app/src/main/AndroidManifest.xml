<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="cn.ololee.examples"
 >
  <!-- android:sharedUserId="android.uid.system"-->

  <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
  <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:requestLegacyExternalStorage="true"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.Examples">
    <activity android:name=".activities.pagedload.PagedLoadedActivity"></activity>
    <activity android:name=".activities.rotate.RotateActivity" />
    <activity android:name=".activities.controller.ControllerActivity" />
    <activity android:name=".activities.watch.TemporatureActivity" />
    <activity android:name=".activities.quadheader.QuadHeadActivity" />
    <activity android:name=".activities.imagesplice.ImageSpliceActivity" />
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>


</manifest>